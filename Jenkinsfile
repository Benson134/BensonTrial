pipeline {
  agent any
  stages {

    stage('Stage 1') {
      steps {
        script {
          echo 'Hello World'
        }
      }
    }

    stage('Stage 2') {
      steps {
        script {
          echo 'Build Hello World'
        }
      }
    }
    stage('Stage 3') {
        steps {
            script {
                echo 'Test Hello World'
            }
        }
    }
    stage('Stage 3') {
        steps {
            script {
                echo 'Deploy Hello World'

  }
}
